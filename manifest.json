{
  "manifest_version": 3,
  "name": "View Time",
  "version": "1.1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "permissions": ["activeTab", "scripting"],

  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_title": "View Time",
    "default_popup": "popup/html.html"
  }
}
